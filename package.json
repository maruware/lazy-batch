    "cp-dts": "cpx dist-tsc/*.d.ts dist",
    "test": "jest"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@types/jest": "^27.0.3",
    "cpx": "^1.5.0",
    "jest": "^27.4.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.2",
    "ts-jest": "^27.1.1",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "just-debounce-it": "^3.0.1"
  }
}
